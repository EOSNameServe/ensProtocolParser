{   //这里必须是2
    "manifest_version": 2,
    //扩展名称
    "name": "ens protocol",
    //扩展版本
    "version": "1.0",
    //扩展描述
    "description": "用于解析 ens 协议页面的跳转",
    //扩展图标(在扩展中心显示)
    "icons": {
        // "16": "images/icon16.png",
        // "48": "images/icon128.png",
        // "128": "images/icon128.png"
    },
    //配置页面
    // "options_page": "options.html",//配置项页面
    //扩展图标配置
    "browser_action": {
        "default_popup": "main.html",
        "default_titlec": "ens 协议解析跳转工具",
        "default_icon": {
            // "16": "images/icon16.png"
        }
    },
    //权限声明
    "permissions":["tabs","https://node.eosflare.io/"]
}